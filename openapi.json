        "/api/reservations": {
            "get": {
                "summary": "Get list of reservation for authenticated user",
                "tags": [
                    "Reservations"
                ],
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "parameters": [
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "items per page",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "page number",
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "description": "filter greater than or equal start_at",
                        "in": "query",
                        "name": "filter[gte_start_at]",
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "description": "filter less than or equal start_at",
                        "in": "query",
                        "name": "filter[lte_start_at]",
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "description": "filter greater than or equal end_at",
                        "in": "query",
                        "name": "filter[gte_end_at]",
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "description": "filter less than or equal end_at",
                        "in": "query",
                        "name": "filter[lte_end_at]",
                        "schema": {
                            "type": "string",
                            "format": "date-time"
                        }
                    },
                    {
                        "description": "sort",
                        "in": "query",
                        "name": "sort[]",
                        "schema": {
                            "description": "sort",
                            "type": "array",
                            "items": {
                                "description": "sort",
                                "type": "string",
                                "enum": [
                                    "-id",
                                    "id",
                                    "-start_at",
                                    "start_at",
                                    "-end_at",
                                    "end_at"
                                ]
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Reservations list",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "description": "response data",
                                    "type": "object",
                                    "required": [
                                        "data",
                                        "links",
                                        "meta"
                                    ],
                                    "properties": {
                                        "data": {
                                            "description": "Reservations list",
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/ReservationIndex"
                                            }
                                        },
                                        "links": {
                                            "description": "pagination links",
                                            "type": "object",
                                            "required": [
                                                "first",
                                                "last",
                                                "prev",
                                                "next"
                                            ],
                                            "properties": {
                                                "first": {
                                                    "type": "string",
                                                    "format": "url",
                                                    "nullable": true
                                                },
                                                "last": {
                                                    "type": "string",
                                                    "format": "url",
                                                    "nullable": true
                                                },
                                                "prev": {
                                                    "type": "string",
                                                    "format": "url",
                                                    "nullable": true
                                                },
                                                "next": {
                                                    "type": "string",
                                                    "format": "url",
                                                    "nullable": true
                                                }
                                            }
                                        },
                                        "meta": {
                                            "description": "pagination meta",
                                            "type": "object",
                                            "required": [
                                                "current_page",
                                                "from",
                                                "last_page",
                                                "path",
                                                "per_page",
                                                "to",
                                                "total"
                                            ],
                                            "properties": {
                                                "current_page": {
                                                    "type": "integer"
                                                },
                                                "from": {
                                                    "type": "integer"
                                                },
                                                "last_page": {
                                                    "type": "integer"
                                                },
                                                "path": {
                                                    "type": "string"
                                                },
                                                "per_page": {
                                                    "type": "integer"
                                                },
                                                "to": {
                                                    "type": "integer"
                                                },
                                                "total": {
                                                    "type": "integer"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "$ref": "#/components/responses/401"
                    },
                    "422": {
                        "$ref": "#/components/responses/422"
                    },
                    "429": {
                        "$ref": "#/components/responses/429"
                    }
                }
            },
            "post": {
                "summary": "Create new reservation for authenticated user",
                "tags": [
                    "Reservations"
                ],
                "security": [
                    {
                        "bearerAuth": []
                    }
                ],
                "requestBody": {
                    "content": {
